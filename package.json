    "dotenv": "^16.4.7",
    "events": "^3.3.0",
    "express": "^4.21.2",
    "glob": "^11.0.0",
    "glob-promise": "^6.0.7",
    "hprose": "^2.0.51",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.46",
